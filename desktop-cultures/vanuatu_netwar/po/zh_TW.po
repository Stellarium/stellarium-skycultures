#. Sky culture name
msgid "Vanuatu (Netwar)"
msgstr "大洋洲 萬那杜 (Netwar)"

#. Vanuatu (Netwar) constellation, native: Kasulia apam
msgid "Long Yoke"
msgstr "長軛"

#. Vanuatu (Netwar) constellation, native: Kasulia rerparep
msgid "Short Yoke"
msgstr "短軛"

#. Vanuatu (Netwar) constellation, native: Kou
msgid "Tweezers for Hot Stones"
msgstr "熱岩夾"

#. Vanuatu (Netwar) constellation, native: Kahaur rul
msgid "Rat"
msgstr "鼠"

#. Vanuatu (Netwar) constellation, native: Kilil
msgid "Fan"
msgstr "風"

#. Vanuatu (Netwar) constellation, native: Suatu kywer
msgid "Four roads"
msgstr "十字路"

#. Vanuatu (Netwar) constellation, native: Nowaswas Lapnuman, Neperawen Lapnuman
msgid "Young Boys from Lapnuman, Young Girls from Lapnuman"
msgstr "Lapnuman的年輕男孩，Lapnuman的年輕女孩"

#. Vanuatu (Netwar) constellation, native: Nowanuman
msgid "Earth oven"
msgstr "土炕窯"

#. Vanuatu (Netwar) constellation, native: Nemrau
msgid "Ashes"
msgstr "灰燼"

#. Vanuatu (Netwar) name for HIP 21421
msgid "Kapaumeta"
msgstr "Kapaumeta"

#. Vanuatu (Netwar) name for NAME Sun, native: Met
msgid "Met"
msgstr "Met - 太陽"

#. Vanuatu (Netwar) name for NAME Moon, native: Mawuk
msgid "Mawuk"
msgstr "Mawuk - 月球"

#. Vanuatu (Netwar) name for NAME Venus, native: Fétukai, Kéwita
msgid "Fétukai (on morning sky), Kéwita (on evening sky)"
msgstr "Fétukai (日出前的金星)，Kéwita (日落後的金星)"

#. Vanuatu (Netwar) name for NAME Jupiter, native: Karatéi
msgid "Karatéi"
msgstr "Karatéi"

#. Vanuatu (Netwar) name for NGC 292
#. Meaning "Earth oven"
msgid "Nowanuman"
msgstr "Nowanuman - 焢窯"

#. Vanuatu (Netwar) name for M 45
#. Meaning "Young boys of Lapnuman"
msgid "Nowaswas Lapnuman"
msgstr "Nowaswas Lapnuman - Lapnuman的年輕男孩"

#. Vanuatu (Netwar) name for M 45
#. Meaning "Young girls of Lapnuman"
msgid "Neperawen Lapnuman"
msgstr "Neperawen Lapnuman - Lapnuman的年輕女孩"
