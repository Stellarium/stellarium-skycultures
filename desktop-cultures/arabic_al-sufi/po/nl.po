"\t<td colspan=\"2\" style=\"text-align:center;\"><b>Khalid</b></td>\n"
"</tr>\n"
"<tr valign=\"top\">\n"
"\t<td><b>eenheid</b></td>\n"
"\t<td><b>ellen</b></td>\n"
"\t<td><b>Hoekwaarde</b></td>\n"
"\t<td><b>ellen</b></td>\n"
"\t<td><b>Hoekwaarde</b></td>\n"
"</tr>\n"
"<tr valign=\"top\">\n"
"\t<td>speer</td>\n"
"\t<td>6 ellen</td>\n"
"\t<td>14°</td>\n"
"\t<td>6 ellen</td>\n"
"\t<td>14°</td>\n"
"</tr>\n"
"<tr valign=\"top\">\n"
"\t<td>Manhoogte</td>\n"
"\t<td></td>\n"
"\t<td></td>\n"
"\t<td>3 ellen</td>\n"
"\t<td>7°</td>\n"
"</tr>\n"
